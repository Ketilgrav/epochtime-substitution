{
	"name": "Epochtime Substitution",
	"version": "0.1",
	"description": "Replaces epoch strings with datetime strings.",
	"homepage_url": "https://github.com/Ketilgrav/epochtime-substitution",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"./substitute.js"
			]
		}
	]
}